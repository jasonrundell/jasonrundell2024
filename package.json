    "isomorphic-dompurify": "^2.30.0",
    "lucide-react": "^0.475.0",
    "next": "^14.2.25",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@contentful/rich-text-react-renderer": "^16.0.0",
